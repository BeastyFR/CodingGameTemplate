{
	"compilerOptions": {
		"module": "amd",
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es6",
		"outFile": "dist/app.js"
	},
	"include": [
		"src/**/*",
	],
	"exclude": [
		"simulation/**/*",
		"tests/**/*",
		"dist/**/*",
		"coverage/**/*"
	]
}